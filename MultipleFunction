  void sum(int ,int );
  void mul(int , int );
  void sub(int ,int );
  void div(int , int );
  void sum(int a ,int b )
  {
    int sum;
    sum=a+b;
   printf("%d",sum);
  }
  void mul(int a, int b)
 {
   int mul;
   mul=a*b;
   printf("mul=%d",mul);
 }
 void sub(int a ,int b)
 {
   int sub;
   sub=a-b;
   printf("sub=%d",sub);
 }
 void div(int a, int b)
 {
   int div;
   div=a/b;
   printf("div=%d",div);
 }

 void main()
 {
   int a ,b,x;
   printf("Enter two number");
   scanf("%d%d",&a,&b);
while(1){
printf("1:sum \n 2:mul \n 3: div \n 4: sub \n");
  scanf("%d",&x);
 
     switch(x)
     { 
     case 1:
  sum(a,b);
   break;
   case 2:
   mul(a,b);
   break;
   case 3:
   div(a,b);
   break;
   case 4:
   sub(a,b);
   break;
   }

   }
 }
